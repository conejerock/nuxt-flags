{
  "name": "nuxt-flags",
  "version": "0.0.3",
  "description": "Nuxt module to use Feature Flags (as Unleash) toggle feature services. Compatible with Nuxt3",
  "repository": "conejerock/nuxt-flags",
  "compatibility": {
    "nuxt": "^3.0.0-rc.1",
    "node": ">=18.19.0"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "i": "yarn install",
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint &&  npm run prepack && npx changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "clean": "yarn cache clean && rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.8.2",
    "colorette": "^2.0.20",
    "consola": "^3.2.3",
    "defu": "^6.1.3",
    "ufo": "^1.3.2",
    "unleash-client": "^5.3.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.0.6",
    "@nuxt/devtools-ui-kit": "^1.0.6",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.4",
    "@nuxt/schema": "^3.8.2",
    "@nuxt/test-utils": "^3.8.1",
    "@types/mock-require": "^2.0.3",
    "@types/node": "^20.10.3",
    "eslint": "^8.55.0",
    "mock-require": "^3.0.3",
    "msw": "^2.0.11",
    "nock": "^13.4.0",
    "vitest": "^0.33.0"
  }
}